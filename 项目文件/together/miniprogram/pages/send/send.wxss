page {
  background-color: #f7f2f2;
}

.normalContent{
  height: 200rpx;
  width: 90%;
  margin: auto;
  margin-top: 35rpx;
  background-color: white;
  border-radius: 20rpx;
  border-bottom: 2rpx solid rgba(128, 128, 128, 0.24);
  border-right: 2rpx solid rgba(128, 128, 128, 0.247);
}

.normalContentText{
  width: 70%;
  font-size: 30rpx;
  line-height: 200rpx;
  float: left;
  text-align: center;
}

.normalContentPicture{
  width: 180rpx;
  height: 140rpx;
  margin-top: 20rpx;
  float: left;
}

.innerPicture{
  width: 100%;
  height: 100%;
}

.line{
  display: none;
  margin: auto;
  width: 90%;
  border-top: 1rpx solid rgba(128, 128, 128, 0.459);
}